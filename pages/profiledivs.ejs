<div id="auto_command_div" style="display:none" method="post">
    <br><br>
    <a class="modern_button" onclick="open_button('auto_command_add_div')" style="--clr:#851804" ><span>add module</span><i></i></a>
    <a class="modern_button" onclick="open_button('auto_command_list_div')" style="--clr:#851804" ><span>list all modules</span><i></i></a>
    <a class="modern_button" onclick="open_button('auto_command_delete_div')" style="--clr:#851804" ><span>delete module</span><i></i></a>

  
        <br>
        <br>
        <br>

    <div class="success-msg" id="auto_command_status" style="display:none">
        <i id="auto_command_message"></i>

    </div>
    <div id="auto_command_add_div" style="display:none" method="post">
        <input class="input-change-password" type=text id="auto_command_title"
        placeholder="title...">
    <input class="input-change-password" type=text id="auto_command_des"
        placeholder="description...">
        <br> <br> <br>
        <textarea id="auto_command_command" class="form-control" placeholder="command.."></textarea>
        <br><br>
        <a class="modern_button" onclick="auto_command_send()" style="--clr:#851804" ><span>Submit </span><i></i></a>

        <br><br><br>

    </div>
    <div id="auto_command_list_div" style="display:none" method="post">
        <table class="modern_table">
            <tr>
                <th>title</th>
                <th>description</th>
                <th>command</th>

            </tr>
            <% for(var i=0; i < auto_command_list.length; i++) { %>
            <tr>
                <td><%= auto_command_list[i].title %></td>
                <td><%= auto_command_list[i].des %></td>
                <td style="word-break:break-all;"><%= auto_command_list[i].command %></td>

            </tr>
            <% } %>
        </table>

    </div>
    <div id="auto_command_delete_div" style="display:none" method="post">
        <input class="input-change-password" type=text id="auto_command_delete_title"
        placeholder="title...">
        <a class="modern_button" onclick="auto_command_delete_send()" style="--clr:#851804" ><span>Submit </span><i></i></a>
        <br><br>
    </div>
</div>
